@model WBD_ASPNET_MVC5.Models.ProjectUsersListViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Add User", "ProjectPage", "AddUser", new { id = Model.project.ID }, null)
</p>
<table class="table">
    <tr>
        <th>
            Username
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.users)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
        </tr>
    }

</table>

